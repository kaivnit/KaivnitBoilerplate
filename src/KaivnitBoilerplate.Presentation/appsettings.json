{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Security": {
    "XFrameOptions": "DENY",
    "ReferrerPolicy": "strict-origin-when-cross-origin",
    "HstsMaxAgeSeconds": "31536000",
    "HstsIncludeSubDomains": "true",
    "HstsPreload": "true",
    "PermissionsPolicy": "geolocation=(), microphone=(), camera=(), payment=(), usb=()",
    "CrossOriginEmbedderPolicy": "require-corp",
    "CrossOriginOpenerPolicy": "same-origin",
    "CrossOriginResourcePolicy": "same-origin",
    "EnableReportTo": "true",
    "ReportToUrl": "/api/csp-report",
    "EnableReportingEndpoints": "true",
    "EnableOriginAgentCluster": "true",
    "DnsPrefetchControl": "off",
    "EnableClearSiteData": "true",
    "EnableNel": "true",
    "NelReportTo": "default",
    "NelMaxAgeSeconds": "86400",
    "TimingAllowOrigin": "*",
    "AcceptClientHints": "DPR, Viewport-Width, Width",
    "CriticalClientHints": "DPR",
    "VaryHeaders": "Accept, Accept-Encoding, Accept-Language, User-Agent, Authorization",
    "XRobotsTag": "noindex, nofollow, noarchive, nosnippet",
    "DocumentPolicy": "force-load-at-top",
    "UpgradeInsecureRequests": "true",
    "PublicCacheMaxAgeSeconds": "3600",
    "EnableRequestTracking": "true",
    "BlockDangerousRequestHeaders": "false",
    "ValidateRequestHeaderValues": "false",
    "MaxRequestHeaderSize": "32768",
    "MaxTotalRequestHeaderSize": "65536"
  }
}
